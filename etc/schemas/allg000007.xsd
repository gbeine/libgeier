	<xs:simpleType name="DatumSType">
		<xs:union memberTypes="elster:allg_DatumSType"/>
	</xs:simpleType>
	<xs:simpleType name="allg_DatumSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Datum und Uhrzeit</xs:documentation>
			<xs:documentation xml:lang="DE">JJJJMMTThhmmss:Zahl, 14-stellig</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{14}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_Datum8SType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Datum 8stellig ohne Trennzeichen</xs:documentation>
			<xs:documentation xml:lang="DE">JJJJMMTT</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}[0-12][0-9][0-3][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="allg_Datum_JahrSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Jahr 4 stellig JJJJ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- E -->
	<xs:simpleType name="allg_ETINSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">die 14stellige eTIN lt. eTIN-Dokumentation</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{8}\d{2}[A-L]{1}[0][1-9][A-Z]{1}"/>
