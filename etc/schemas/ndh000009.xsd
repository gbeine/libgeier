	<xs:complexType name="EmpfaengerCType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Ob ein Empfänger ein Finanzamt oder ein Bundesland ist, ist vom Verfahren abhängig</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="elster:EmpfaengerSType">
				<xs:attribute name="id" type="elster:EmpfaengerIDSType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!-- F -->
	<!-- G -->
	<!-- H -->
	<xs:complexType name="HerstellerCType">
		<xs:sequence>
			<xs:element name="ProduktName">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ProduktVersion">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- J -->
	<!-- K -->
	<!-- L -->
	<!-- M -->
	<!-- N -->
	<!-- O -->
	<!-- P -->
	<!-- Q -->
