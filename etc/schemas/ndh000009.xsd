	<!-- A -->
	<!-- B -->
	<!-- C -->
	<xs:simpleType name="CodeExternSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Externer Fehlercode</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="CodeInternSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Interner Fehlercode</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!-- D -->
	<xs:simpleType name="DatenLieferantSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Datenlieferant der Nutzdaten - Arbeitgeber selbst bzw. dessen Lohnbuchhhaltungsbüro / Steuerberater</xs:documentation>
			<xs:documentation xml:lang="DE">String der Lieferanteninformationen enthält ; Ansprechpartner mit Telephon. Email etc. Bei ESt: Steuerpflichtiger oder Steuerberater/Firma  (entfällt hier u.U. auch)Aufbau: Lieferant (Firma); Ansprechpartner; Telefon; E-Mail Addresse; Ort; PLZ; Strasse; Land;</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="256"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- E -->
	<xs:simpleType name="EmpfaengerSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Bundeslandkürzel oder Finanzamtsnummer</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="elster:ndh_EmpfaengerLISTE elster:ndh_Zahl4LISTE"/>
	</xs:simpleType>
	<xs:simpleType name="ndh_EmpfaengerLISTE">
		<xs:list itemType="elster:BundeslandSType"/>
	</xs:simpleType>
	<xs:simpleType name="ndh_Zahl4LISTE">
		<xs:list itemType="elster:Zahl4"/>
	</xs:simpleType>
	<xs:simpleType name="EmpfaengerIDSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">
							Attribut, ob Länderschlüssel oder BundesFinanzamtsnummer
							L= Länderschlüssel (z.B. bei LStB.)
							F= BundesFinanzamtsnummer (z.B. bei ESt)
						</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="F"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- F -->
	<!-- G -->
	<!-- H -->
	<xs:simpleType name="HerstellerIDSType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{5}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- I -->
	<!-- J -->
