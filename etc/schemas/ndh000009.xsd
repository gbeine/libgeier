	<xs:complexType name="RCCType">
		<xs:sequence minOccurs="0">
			<xs:sequence minOccurs="0">
				<xs:element name="SigIntern" type="elster:SigInternSType"/>
				<xs:element name="VerarbeitungsVersion" type="xs:string"/>
			</xs:sequence>
			<xs:element name="Rueckgabe">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" type="elster:CodeExternSType"/>
						<xs:element name="Text" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Stack">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Code" type="elster:CodeInternSType"/>
						<xs:element name="Text" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- S -->
	<xs:complexType name="SigUserCType">
		<xs:sequence>
			<xs:element name="SigPruefung" type="elster:SigPruefungSType" minOccurs="0"/>
			<xs:element name="UserInfo" type="elster:UserInfoCType"/>
			<xs:element name="Sig" type="elster:SigSType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- T -->
	<!-- U -->
	<xs:complexType name="UserInfoCType">
		<xs:sequence>
			<xs:element name="User" type="elster:UserCType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UserCType">
		<xs:sequence>
			<xs:element name="Vorname" type="xs:string" minOccurs="0"/>
			<xs:element name="Name" type="xs:string"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<!-- V -->
	<!-- W -->
	<!-- X -->
	<!-- Y -->
	<!-- Z -->
	<xs:complexType name="ZusatzCType">
		<xs:sequence>
			<xs:element name="Info" type="xs:string" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
