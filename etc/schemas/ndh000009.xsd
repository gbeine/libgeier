	<xs:complexType name="SigSType" mixed="true">
		<xs:complexContent mixed="true">
			<xs:restriction base="xs:anyType">
				<xs:sequence>
					<xs:any processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="SigPruefungSType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{14}"/>
			<xs:pattern value="a?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SigInternSType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
