	<!-- A -->
	<!-- B -->
	<xs:simpleType name="BaseStringSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">die Einschränkungen sind in der Schnittstellenbeschreibung SST_Arbeitgeber zu entnehmen"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringNoLeadingOrTrailingSeparators">
			<xs:pattern value="[\p{L}|\p{P}|\p{Zs}|\p{Nd}]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringLightSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">Der String muss mindestens 1 Zeichen vom Typ {N} oder {L} beinhalten, zusätzlich gelten die Einschränkungen  des BaseStringNoLeadingOrTrailingSeparators</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringNoLeadingOrTrailingSeparators">
			<xs:pattern value=".*([\p{N}]|[\p{L}]).*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringNoLeadingOrTrailingSeparators">
		<xs:annotation>
			<xs:documentation xml:lang="DE">das letzte Zeichen darf nicht vom Typ {Z} sein, zusätzlich gelten die Einschränkungen  des BaseStringNoLeadingSeparators</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringNoLeadingSeparators">
			<!-- 
			  modified for purposes of libgeier, original:
			  <xs:pattern value=".*[\P{Z}]"/>
			-->
			<xs:pattern value=".*[^\p{Z}]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringNoLeadingSeparators">
		<xs:annotation>
			<xs:documentation xml:lang="DE">das erste Zeichen darf nicht vom Typ {Z} sein, zusätzlich gelten die Einschränkungen  des BaseStringUltraLightSType</xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="elsterheaderbasis:BaseStringUltraLightSType">
			<!-- 
			  modified for purposes of libgeier, original:
			  <xs:pattern value="[\P{Z}].*"/> 
			-->
			<xs:pattern value="[^\p{Z}].*" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaseStringUltraLightSType">
		<xs:annotation>
			<xs:documentation xml:lang="DE">alle Zeichen die nicht Kategorien C oder Zl gehören </xs:documentation>
			<xs:documentation xml:lang="DE">Verweis auf http://www.w3.org/TR/2001/REC-xmlschema-2-20010502/#regexs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<!-- 
			  modified for purposes of libgeier, original:
			  <xs:pattern value="([^\n\r\t\p{C}\p{Zl}])*"/>
			-->
			<xs:pattern value="([^\p{C}\p{Zl}])*"/>
		</xs:restriction>
	</xs:simpleType>
