<xs:schema targetNamespace="http://www.elster.de/2002/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:elster="http://www.elster.de/2002/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="NutzdatenHeader" type="elster:NutzdatenHeaderCType"/>
	<xs:complexType name="NutzdatenHeaderCType">
		<xs:sequence>
			<xs:element name="NutzdatenTicket" type="elster:NutzdatenBlockTicketSType"/>
			<xs:element name="Empfaenger" type="elster:EmpfaengerCType"/>
			<xs:element name="SigUser" type="elster:SigUserCType" minOccurs="0"/>
			<xs:element name="Hersteller" type="elster:HerstellerCType"/>
			<xs:element name="DatenLieferant" type="elster:DatenLieferantSType"/>
			<xs:element name="RC" type="elster:RCCType" minOccurs="0"/>
			<xs:element name="Zusatz" type="elster:ZusatzCType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="version" type="elster:VersionSType" use="required"/>
	</xs:complexType>
